{
    "root": true,
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "plugins": ["jsx-a11y", "import", "react"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true
    },
    "rules": {
        "react/prop-types": "off"
    }
}
