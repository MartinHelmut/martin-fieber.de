{
  "name": "martin-fieber.se",
  "version": "1.0.0",
  "description": "My personal site, portfolio, experiments or other stuff I wanted to try.",
  "private": true,
  "scripts": {
    "start": "serve src",
    "commit": "git-cz",
    "lint": "eslint '**/*.html' && stylelint '**/*.css'",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "ci": "pnpm run lint && pnpm run format:check",
    "preinstall": "node ./scripts/ensure-package-manager.js"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHelmut/martin-fieber.de.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "martin",
    "helmut",
    "fieber"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.se> (https://martin-fieber.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/martin-fieber.de/issues"
  },
  "homepage": "https://martin-fieber.se/",
  "dependencies": {},
  "devDependencies": {
    "commitizen": "4.2.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.1.2",
    "prettier": "2.3.2",
    "serve": "12.0.0",
    "stylelint": "13.13.1",
    "stylelint-prettier": "1.2.0"
  }
}
