{
  "name": "martin-fieber.se",
  "version": "1.0.0",
  "description": "My personal site, portfolio, experiments or other stuff I wanted to try.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export",
    "dev": "next",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.jsx'",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,jsx,css,md,json,yml}\"",
    "ci": "yarn lint && yarn format:check && yarn test && yarn coverage",
    "preinstall": "node ./scripts/ensure-package-manager.js"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHelmut/martin-fieber.de.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "martin",
    "helmut",
    "fieber"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.se> (https://martin-fieber.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/martin-fieber.de/issues"
  },
  "homepage": "https://martin-fieber.se/",
  "dependencies": {
    "next": "9.5.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-is": "16.13.1",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/preset-env": "7.12.0",
    "@babel/preset-react": "7.10.4",
    "babel-jest": "26.5.2",
    "babel-plugin-styled-components": "1.11.1",
    "commitizen": "4.2.1",
    "coveralls": "3.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "jest": "26.5.3",
    "jest-progress-bar-reporter": "1.0.18",
    "jest-styled-components": "7.0.3",
    "prettier": "2.1.2",
    "react-test-renderer": "16.14.0",
    "standard-version": "9.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
