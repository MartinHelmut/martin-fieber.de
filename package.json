{
  "name": "martin-fieber.se",
  "version": "1.0.0",
  "description": "My personal site, portfolio, blog, experiments or other things I wanted to try.",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy && prettier --write _site --ignore-path .prettierignore",
    "lint": "eslint src -c eslint.config.js && stylelint '**/*.css'",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "compress": "./tools/compress.sh"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHelmut/martin-fieber.de.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "martin",
    "helmut",
    "fieber"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.se> (https://martin-fieber.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/martin-fieber.de/issues"
  },
  "homepage": "https://martin-fieber.se/",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.18",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@eslint/js": "^9.9.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "globals": "^15.9.0",
    "luxon": "^3.5.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-prettier": "^5.0.2",
    "terser": "^5.33.0"
  }
}
