{
  "name": "martin-fieber.de",
  "version": "1.0.0",
  "description": "My personal website.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export -o public",
    "postbuild": "cp deployment/* public/",
    "dev": "next",
    "test": "jest",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.css'",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,jsx,css,md,json,yml}\"",
    "ci": "npm run lint && npm run format:check && npm test"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHelmut/martin-fieber.de.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "martin",
    "helmut",
    "fieber"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/martin-fieber.de/issues"
  },
  "homepage": "https://martin-fieber.de/",
  "dependencies": {
    "next": "7.0.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "acorn": "6.0.6",
    "babel-jest": "24.0.0",
    "babel-plugin-styled-components": "1.10.0",
    "commitizen": "3.0.5",
    "eslint": "5.12.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.0.0",
    "lint-staged": "8.1.1",
    "prettier": "1.16.3",
    "react-test-renderer": "16.7.0",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2"
  }
}
