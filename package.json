{
  "name": "martin-fieber.de",
  "version": "0.1.0",
  "description": "My personal website.",
  "private": true,
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "commit": "node node_modules/@commitlint/prompt-cli/cli.js",
    "commitmsg": "node node_modules/@commitlint/cli/lib/cli.js -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js,.jsx src && stylelint 'src/*.css'",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "fmt:check": "prettier --tab-width 4 --single-quote --list-different 'src/**/*.{js,jsx}'",
    "clean": "rm -rf public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHelmut/martin-fieber.de.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "martin",
    "helmut",
    "fieber"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/martin-fieber.de/issues"
  },
  "homepage": "https://martin-fieber.de/",
  "dependencies": {
    "gatsby": "1.9.247",
    "gatsby-cli": "1.1.50",
    "gatsby-image": "1.0.43",
    "gatsby-link": "1.6.40",
    "gatsby-plugin-canonical-urls": "1.0.16",
    "gatsby-plugin-google-analytics": "1.0.28",
    "gatsby-plugin-netlify": "1.0.19",
    "gatsby-plugin-react-helmet": "2.0.10",
    "gatsby-plugin-remove-trailing-slashes": "1.0.9",
    "gatsby-plugin-sharp": "1.6.41",
    "gatsby-plugin-sitemap": "1.2.20",
    "gatsby-plugin-typography": "1.7.18",
    "gatsby-transformer-sharp": "1.6.22",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-helmet": "5.2.0",
    "typography": "0.16.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "6.1.3",
    "@commitlint/cli": "6.1.3",
    "@commitlint/prompt-cli": "6.1.3",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.4",
    "prettier": "1.11.1",
    "react-test-renderer": "16.3.1",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --tab-width 4 --single-quote",
      "git add"
    ],
    "*.{md,json}": [
      "prettier --write --tab-width 4",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "node": true,
      "jest": true,
      "browser": true
    },
    "rules": {
      "react/prop-types": 0,
      "react/jsx-indent": [
        1,
        4
      ],
      "react/jsx-indent-props": [
        1,
        4
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "a"
          ]
        }
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 4
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>src/utils/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>/src/utils/"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "dependencies": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "pin": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": ""
  }
}
