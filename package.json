{
  "name": "martin-fieber.de",
  "version": "1.0.0",
  "description": "My personal website.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export",
    "postbuild": "cp deployment/* out/",
    "dev": "next",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.jsx'",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,jsx,css,md,json,yml}\"",
    "ci": "yarn lint && yarn format:check && yarn test && yarn coverage",
    "release": "standard-version -a -s",
    "preinstall": "node ./scripts/ensure-package-manager.js"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHelmut/martin-fieber.de.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "martin",
    "helmut",
    "fieber"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/martin-fieber.de/issues"
  },
  "homepage": "https://martin-fieber.de/",
  "dependencies": {
    "next": "9.1.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "babel-jest": "24.9.0",
    "babel-plugin-styled-components": "1.10.6",
    "commitizen": "3.1.1",
    "coveralls": "3.0.7",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "jest-dot-reporter": "1.0.10",
    "jest-styled-components": "6.3.3",
    "prettier": "1.18.2",
    "react-test-renderer": "16.11.0",
    "standard-version": "7.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0"
  }
}
